-- 테이블 root.boardfile 구조 내보내기
CREATE TABLE IF NOT EXISTS `boardfile` (
  `f_idx` int(11) NOT NULL AUTO_INCREMENT,
  `filename` varchar(255) DEFAULT NULL,
  `orgFileName` varchar(255) DEFAULT NULL,
  `b_idx` int(11) NOT NULL,
  PRIMARY KEY (`f_idx`),
  KEY `FK_boardfile_root.board` (`b_idx`),
  CONSTRAINT `FK_boardfile_root.board` FOREIGN KEY (`b_idx`) REFERENCES `board` (`b_idx`) ON DELETE NO ACTION ON UPDATE NO ACTION
) ENGINE=InnoDB AUTO_INCREMENT=9 DEFAULT CHARSET=utf8;

